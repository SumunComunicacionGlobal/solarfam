{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,

		"color": {
			"custom": false,
			"defaultPalette": false,
			"defaultGradients": false,
			"customDuotone": false,
			"customGradient": false,
			"palette": [
				{
					"slug": "background",
					"color": "#FFFFFF",
					"name": "Background"
				},
				{
					"slug": "primary-10",
					"color": "#FFEAE7",
					"name": "Primary 10"
				},
				{
					"slug": "primary-30",
					"color": "#FFA498",
					"name": "Primary 30"
				},
				{
					"slug": "primary",
					"color": "#FF2B0F",
					"name": "Primary"
				},
				{
					"slug": "primary-80",
					"color": "#B51F0B",
					"name": "Primary 80"
				},
				{
					"slug": "primary-100",
					"color": "#731307",
					"name": "Primary 100"
				},
                {
					"slug": "secondary-10",
					"color": "#FFF0E7",
					"name": "Secondary 10"
				},
				{
					"slug": "secondary-30",
					"color": "#FFBD97",
					"name": "Secondary 30"
				},
				{
					"slug": "secondary",
					"color": "#FF660D",
					"name": "Secondary"
				},
				{
					"slug": "secondary-70",
					"color": "#D9570B",
					"name": "Secondary 70"
				},
				{
					"slug": "secondary-80",
					"color": "#B54809",
					"name": "Secondary 80"
				},
				{
					"slug": "secondary-100",
					"color": "#732E06",
					"name": "Secondary 100"
				},
                {
					"slug": "accent-10",
					"color": "#E6EDF7",
					"name": "Accent 10"
				},
				{
					"slug": "accent-30",
					"color": "#92B1DE",
					"name": "Accent 30"
				},
				{
					"slug": "accent",
					"color": "#0249B3",
					"name": "Accent"
				},
				{
					"slug": "accent-80",
					"color": "#01347F",
					"name": "Accent 80"
				},
				{
					"slug": "accent-90",
					"color": "#012A66",
					"name": "Accent 90"
				},
				{
					"slug": "accent-100",
					"color": "#012151",
					"name": "Accent 100"
				},
				{
					"slug": "text-medium",
					"color": "#4D5963",
					"name": "Text medium"
				},
				{
					"slug": "text-body",
					"color": "var(--wp--custom--color--neutral-90)",
					"name": "Text body"
				},
				{
					"slug": "white-20",
					"color": "rgba(255, 255, 255, 0.20)",
					"name": "White 20"
				},
				{
					"slug": "neutral-10",
					"color": "var(--wp--custom--color--neutral-10)",
					"name": "Neutral 10"
				}
			],
			"gradients": [],
			"link": false
		},
		"custom": {
			"border-radius": {
				"zero": "0",
				"sm": "4px",
				"md": "8px",
                "lg": "16px",
                "xl": "32px",
				"rounded": "999px"
			},
			"border-width": {
				"tiny": "1px",
				"small": "2px",
				"medium": "3px",
				"large": "4px"
			},
			"box-shadow": {
				"1": "0px 2px 8px rgba(33, 33, 33, 0.12)",
				"2": "3px 3px 10px 10px rgba(0, 0, 0, 0.10)"
			},
			"color": {
				"link" : "var(--wp--preset--color--primary)",
				"neutral-white": "#FFFFFF",
				"neutral-10": "#F2F2F2",
				"neutral-20": "#D4D5D4",
				"neutral-30": "#B7B9B6",
				"neutral-40": "#999B98",
				"neutral-50": "#7C7E7B",
				"neutral-60": "#636563",
				"neutral-70": "#4B4C4B",
				"neutral-80": "#323332",
				"neutral-90": "#191919",
				"neutral-100": "#010101",
				"neutral-black": "#000000"
			},
			"layout": {
				"content": "1312px",
				"wide": "1280px",
				"sidebar": "336px",
				"page": "var(--wp--custom--layout--content)",
				"padding": "16px",
				"block-gap": "2rem",
				"block-gap-large": "40px"
			},
			"letter-spacing": {
				"none": "normal",
				"tight": "-.01em",
				"loose": ".05em",
				"looser": ".1em"
			},
			"line-height": {
				"tiny": 1,
				"headings": 1,
				"normal": 1.33
			}
		},
		"spacing": {
			"blockGap": true,
			"margin": true,
			"padding": true,
			
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],

			"spacingSizes": [
				{
					"size": "1rem",
					"slug": "10",
					"name": "Space 1 (1rem - 16px)"
				},

				{
					"size": "1.5rem",
					"slug": "20",
					"name": "Space 2 (1.5rem - 24px)"
				},
				{
					"size": "2rem",
					"slug": "30",
					"name": "Space 3 (2rem - 32px)"
				},
				{
					"size": "3rem",
					"slug": "40",
					"name": "Space 4 (3rem - 48px)"
				},
				{
					"size": "4rem",
					"slug": "60",
					"name": "Space 6 (4rem - 64px)"
				},
				{
					"size": "4.5rem",
					"slug": "70",
					"name": "Space 7 (4.5rem - 72px)"
				},
				{
					"size": "5.5rem",
					"slug": "80",
					"name": "Space 8 (5.5rem - 88px)"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"lineHeight": false,
			"customFontSize": false,
			"fontWeight": true,
			"letterSpacing": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontDisplay": "block",
							"fontFamily": "PP Neue Montreal Thin",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/ppneuemontreal-thin.woff"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "PP Neue Montreal Book",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ppneuemontreal-book.woff"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "PP Neue Montreal Italic",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ppneuemontreal-italic.woff"
							]
						},
						{
							"fontDisplay": "block",
							"fontFamily": "PP Neue Montreal Bold",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/ppneuemontreal-bold.woff"
							]
						}
					],
					"fontFamily": "\"PP Neue Montreal Book\", sans-serif",
					"name": "Main Font",
					"slug": "main-font"
				}
			],
            
			"fontSizes": [
				{
					"name": "Display",
					"size": "clamp(3rem, 8.8vw, 8rem)",
					"slug": "display"
				},
				{
					"name": "H1",
					"size": "clamp(3rem, 4.8vw, 4rem)",
					"slug": "heading-1"
				},
				{
					"name": "H2",
					"size": "clamp(2rem, 4.4vw, 3rem)",
					"slug": "heading-2"
				},
				{
					"name": "H3",
					"size": "2rem",
					"slug": "heading-3"
				},
				{
					"name": "H4",
					"size": "1.5rem",
					"slug": "heading-4"
				},
				{
					"name": "H5",
					"size": "1.25rem",
					"slug": "heading-5"
				},
				{
					"name": "H6",
					"size": "1.125",
					"slug": "heading-6"
				},
				{
					"name": "Subtitle",
					"size": "clamp(0.875rem, 4.4vw, 1.5rem)",
					"slug": "subtitle"
				},
				{
					"name": "Body",
					"size": "1rem",
					"slug": "body"
				},
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Caption",
					"size": "0.75rem",
					"slug": "caption"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content)",
			"wideSize": "var(--wp--custom--layout--wide)"
		},
		"border": {
			"color": false,
			"radius": true,
			"style": false,
			"width": false
		},
		"blocks": {
			"core/button": {
				"border": {
					"radius": true
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--custom--color--neutral-90)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--custom--color--link)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"border": {
					"width": "0px",
					"radius": "0"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--body)",
                    "fontFamily": "var(--wp--preset--font-family--main-font)",
					"lineHeight": "var(--wp--custom--line-height--normal)"
				},
				"spacing": {
					"padding": "calc(0.5rem + 4px) 1.5em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--main-font)",
					"lineHeight": "var(--wp--custom--line-height--headings)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-1)",
					"lineHeight": "var(--wp--custom--line-height--tiny)",
					"fontWeight": "200"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-2)",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-3)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-4)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--heading-6)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--layout--block-gap)",
			"padding": {
                "top": "0px",
                "right": "2rem",
                "bottom": "0px",
                "left": "2rem"
            }
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--main-font)",
			"lineHeight": "var(--wp--custom--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--body)"
		},

		"blocks": {
			"core/group": {
				"spacing": {
				}
			}
		}
	}
}
